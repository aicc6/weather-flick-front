# Weather Flick Frontend Environment Variables
# Copy this file to .env and fill in the values

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Maps Map ID (for AdvancedMarkerElement)
# Leave empty to use DEMO_MAP_ID for development
VITE_GOOGLE_MAPS_MAP_ID=your_google_maps_map_id_here

# Backend API URL
VITE_API_BASE_URL=http://localhost:8000

# Google OAuth Client ID
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Application Environment
VITE_APP_ENV=development

# Enable/Disable Features
VITE_ENABLE_GOOGLE_MAPS=true
VITE_ENABLE_SOCIAL_LOGIN=true

# Pixabay API Key (for region images)
VITE_PIXABAY_API_KEY=your_pixabay_api_key_here

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_firebase_measurement_id
VITE_FIREBASE_VAPID_KEY=your_firebase_vapid_key
