# Weather Flick Frontend Environment Variables
# Copy this file to .env and fill in the values

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Maps Map ID (for AdvancedMarkerElement)
# Leave empty to use DEMO_MAP_ID for development
VITE_GOOGLE_MAPS_MAP_ID=your_google_maps_map_id_here

# Backend API URL
VITE_API_BASE_URL=http://localhost:8000

# Google OAuth Client ID
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Application Environment
VITE_APP_ENV=development

# Enable/Disable Features
VITE_ENABLE_GOOGLE_MAPS=true
VITE_ENABLE_SOCIAL_LOGIN=true